                  {
                     "type": "latchstep",
                     "minOutputTime": 6000
                  }
               ]
            },
            {
               "name": "daylight-threshold",
               "type": "thresholdproperty",
               "initialValue": false,
               "writable": false,
               "threshold" : 47,
               "buffer": 7,
               "transform": "$value>=47",
               "source": {
                  "property": "light-sensor"
               }
            },
            {
               "name": "low-light-threshold",
               "initialValue": false,
               "writable": false,
               "type": "thresholdproperty",
               "threshold" : 63,
               "buffer": 7,
               "transform": "$value<=63",
               "source": {
                  "property": "light-sensor"
               }
            },
            {
               "name": "low-light-kitchen",
               "initialValue": false,
               "writable": false,
               "type": "thresholdproperty",
               "threshold" : 68,
               "buffer": 7,
               "transform": "$value<=68",
               "source": {
                  "property": "light-sensor"
               }
            },
            {
               "name": "daylight-possible",
               "initialValue": false,
               "writable": false,
               "type": "scheduleproperty",
               "events": [
                  {
                     "rule": "sunrise:-600",
                     "value": true
                  },
                  {
                     "rule": "sunset:+600",
                     "value": false
                  }
               ]
            },
            {
               "name": "daylight",
               "initialValue": false,
               "writable": false,
               "type": "andproperty",
               "sources": [
                  {
                     "property": "daylight-threshold"
                  },
                  {
                     "property": "daylight-possible"
                  }
               ]
            },
            {
               "name": "dark",
               "initialValue": false,
               "writable": false,
               "sources": [
                  {
                     "property": "daylight",
                     "transform": "!$value"
